open Generation
open Affichage

(*********************************************)
(*                GESTION DU JEU             *)
(*********************************************)

type direction = North | South | East | West
let deplace_nord (x,y) = (x, y+1)
let deplace_sud  (x,y) = (x, y-1)
let deplace_est  (x,y) = (x+1, y)
let deplace_west (x,y) = (x-1, y)

